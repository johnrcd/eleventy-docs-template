		"url": "https://rovidecena.com/"
	},
	"license": "MIT",
	"engines": {
		"node": ">=18"
	},
	"homepage": "https://github.com/johnrcd/eleventy-docs-template",
	"devDependencies": {
		"@11ty/eleventy": "^3.1.0"
	},
	"dependencies": {
		"@11ty/eleventy-navigation": "^1.0.5",
		"@zachleat/pagefind-search": "^1.0.3",
		"markdown-it": "^14.1.0",
		"markdown-it-admon": "^1.0.1",
		"markdown-it-anchor": "^9.2.0",
		"yaml": "^2.8.2"
	}
}