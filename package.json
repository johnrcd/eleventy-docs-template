{
	"name": "eleventy-docs-template",
	"version": "1.0.0",
	"description": "A starter repository for a documentation website using the Eleventy site generator.",
	"type": "module",
	"scripts": {
		"dev": "npx @11ty/eleventy --serve --quiet",
		"index": "npx -y pagefind --site _site",
		"build": "build:*",
		"build:11ty": "eleventy",
		"build:index": "npx -y pagefind --site _site"
	},
	"author": {
		"name": "Rovi Decena",
